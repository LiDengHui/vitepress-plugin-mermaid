{
  "name": "vitepress-plugin-mermaid2",
  "version": "0.0.1",
  "description": "Add mermaid support for Vitepress",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lidenghui/vitepress-plugin-mermaid.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/vitepress-plugin-mermaid.umd.js",
  "module": "./dist/vitepress-plugin-mermaid.es.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vitepress-plugin-mermaid.es.mjs",
      "require": "./dist/vitepress-plugin-mermaid.umd.js"
    },
    "./Mermaid.vue": "./dist/Mermaid.vue"
  },
  "keywords": [
    "vitepress",
    "mermaid"
  ],
  "author": "lidenghui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LiDengHui/vitepress-plugin-mermaid/issues"
  },
  "homepage": "https://github.com/LiDengHui/vitepress-plugin-mermaid#readme",
  "peerDependencies": {
    "mermaid": "10 || 11",
    "vitepress": "^2.0.0-beta || ^2.0.0"
  },
  "optionalDependencies": {
    "@mermaid-js/mermaid-mindmap": "^9.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "markdown-it": "^14.1.0",
    "prettier": "^3.6.2",
    "vite": "^5",
    "vite-plugin-dts": "^3.0.3",
    "vite-plugin-static-copy": "^3.1.1",
    "vitepress": "^2.0.0-alpha.0"
  }
}
